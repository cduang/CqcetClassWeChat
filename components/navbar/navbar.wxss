/* navbar.wxss */

.nav-wrapper {
  width: 100%;
  position: relative;
  z-index: 10;
}
.title {
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  font-size:36rpx;
  letter-spacing:2px;
  color: white;
}
.login-icon {
  width: 22px;
  height: 22px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
  background-size: cover;
  z-index: 11;
  background-image: url("data:image/svg+xml;base64,");
}

.back-icon {
  width: 22px;
  height: 22px;
  color: white;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
  background-size: 30px;
  background-repeat: no-repeat;
  background-position: center center;
  z-index: 11;
  background-image: url("data:image/svg+xml;base64,");
}
