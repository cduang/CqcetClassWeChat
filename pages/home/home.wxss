/* home.wxss */
@import "./weSwiper.wxss";

Page{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
}
.course-panles-wrapper {
  box-sizing: border-box;
}

.course-item-scroll {
  width: 100%;
  height: 100%;
}

.tips {
  margin-top: 20%;
  display: flex;
  justify-content: center;
  height: 240px;
  width: 100%;
  text-align: center;
  color: darkgray;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position: center;
}
.tips > .tips-content {
  font-size: 15px;
  height: 40px;
  width: 200px;
}


